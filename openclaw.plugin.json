{
  "id": "qmd-memory",
  "name": "QMD Memory",
  "description": "QMD-backed memory plugin for OpenClaw",
  "kind": "memory",
  "main": "plugin/index.ts",
  "configSchema": {
    "type": "object",
    "properties": {
      "index": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "default": "memory"
          },
          "dbPath": {
            "type": "string"
          },
          "reuseQmdCache": {
            "type": "boolean",
            "default": false
          }
        },
        "additionalProperties": true
      },
      "collections": {
        "type": "object",
        "additionalProperties": true
      },
      "models": {
        "type": "object",
        "properties": {
          "mode": {
            "type": "string",
            "enum": [
              "local",
              "api"
            ],
            "default": "api"
          },
          "api": {
            "type": "object",
            "properties": {
              "embedding": {
                "type": "object",
                "properties": {
                  "provider": {
                    "type": "string",
                    "default": "openai-compatible"
                  },
                  "timeoutMs": {
                    "type": "number",
                    "default": 30000
                  },
                  "baseUrl": {
                    "type": "string"
                  },
                  "apiKeyEnv": {
                    "type": "string"
                  },
                  "model": {
                    "type": "string"
                  }
                },
                "additionalProperties": true
              },
              "rerank": {
                "type": "object",
                "properties": {
                  "provider": {
                    "type": "string",
                    "default": "openai-compatible"
                  },
                  "timeoutMs": {
                    "type": "number",
                    "default": 30000
                  },
                  "baseUrl": {
                    "type": "string"
                  },
                  "apiKeyEnv": {
                    "type": "string"
                  },
                  "model": {
                    "type": "string"
                  }
                },
                "additionalProperties": true
              },
              "generate": {
                "type": "object",
                "properties": {
                  "provider": {
                    "type": "string",
                    "default": "openai-compatible"
                  },
                  "timeoutMs": {
                    "type": "number",
                    "default": 30000
                  },
                  "baseUrl": {
                    "type": "string"
                  },
                  "apiKeyEnv": {
                    "type": "string"
                  },
                  "model": {
                    "type": "string"
                  }
                },
                "additionalProperties": true
              }
            },
            "additionalProperties": true
          }
        },
        "additionalProperties": true
      },
      "sync": {
        "type": "object",
        "properties": {
          "enabled": {
            "type": "boolean",
            "default": true
          },
          "intervalSec": {
            "type": "number",
            "default": 300
          },
          "embedOnSync": {
            "type": "boolean",
            "default": false
          }
        },
        "additionalProperties": true
      }
    },
    "additionalProperties": true
  }
}
